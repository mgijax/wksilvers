# Read-only Sybase user.
DBUSER		mgd_public

# Password for the read-only Sybase user.
DBPASSWORD	mgdpub

# Name of the Sybase server 
DBSERVER	

# Name of Sybase database
DATABASE	

# Title to be displayed on dynamically created gene/allele data pages
WKSTITLE	MGI - The Coat Colors of Mice by WK Silvers

# Path(s) to external (non-WKS) library files.  Multiple directories may be
# included by separating them with colons.  Mirror sites should direct this
# to your mgi/lib/ directory.
LIBDIRS		/usr/local/mgi/lib/python/

# Path to compiled python configuration file.
# Example-> /usr/local/mgi/lib/python/Configuration.pyc 
CONFIGPATH 	/usr/local/mgi/lib/python/Configuration.pyc

# Path to the python executable
# Example-> /usr/local/bin/python
PYTHONPATH 	/usr/local/bin/python


# Base URL to the book pages
WKSBOOKURL      http://

# Full URL coat:color anomalies
COATANOM        http://www.informatics.jax.org/searches/marker_report.cgi?phenoslim=17

# Base URL to the MGI searches
MGISEARCHES	http://www.informatics.jax.org/searches/

#####################################################################
# The following parameters are used in the gen_include script 
INCACCRPT	${MGISEARCHES}accession_report.cgi?id=

INCALLRPT	${MGISEARCHES}allele_report.cgi?markerID=

INCPUBMED	http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Abstract&list_uids=

INCUSRSUP	http://www.informatics.jax.org/mgihome/help/help.shtml

INCPEOPLE	http://www.informatics.jax.org/mgihome/other/mgi_people.shtml

INCHELPMAIL	http://www.informatics.jax.org/mgihome/support/tjl_inbox.shtml

INCGLOSSARY	http://www.informatics.jax.org/mgihome/other/glossary.shtml
